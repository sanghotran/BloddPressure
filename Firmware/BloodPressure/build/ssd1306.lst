 200:lib/ssd1306/ssd1306.c **** //    Draw one pixel in the screenbuffer
 201:lib/ssd1306/ssd1306.c **** //    X => X Coordinate
 202:lib/ssd1306/ssd1306.c **** //    Y => Y Coordinate
 203:lib/ssd1306/ssd1306.c **** //    color => Pixel color
 204:lib/ssd1306/ssd1306.c **** void ssd1306_DrawPixel(uint8_t x, uint8_t y, SSD1306_COLOR color) {
 205:lib/ssd1306/ssd1306.c ****     if(x >= SSD1306_WIDTH || y >= SSD1306_HEIGHT) {
 206:lib/ssd1306/ssd1306.c ****         // Don't write outside the buffer
 207:lib/ssd1306/ssd1306.c ****         return;
 208:lib/ssd1306/ssd1306.c ****     }
 209:lib/ssd1306/ssd1306.c ****    
 210:lib/ssd1306/ssd1306.c ****     // Draw in the right color
 211:lib/ssd1306/ssd1306.c ****     if(color == White) {
 212:lib/ssd1306/ssd1306.c ****         SSD1306_Buffer[x + (y / 8) * SSD1306_WIDTH] |= 1 << (y % 8);
 213:lib/ssd1306/ssd1306.c ****     } else { 
 214:lib/ssd1306/ssd1306.c ****         SSD1306_Buffer[x + (y / 8) * SSD1306_WIDTH] &= ~(1 << (y % 8));
 215:lib/ssd1306/ssd1306.c ****     }
 216:lib/ssd1306/ssd1306.c **** }
 217:lib/ssd1306/ssd1306.c **** 
 218:lib/ssd1306/ssd1306.c **** // Draw 1 char to the screen buffer
 219:lib/ssd1306/ssd1306.c **** // ch       => char om weg te schrijven
 220:lib/ssd1306/ssd1306.c **** // Font     => Font waarmee we gaan schrijven
 221:lib/ssd1306/ssd1306.c **** // color    => Black or White
 222:lib/ssd1306/ssd1306.c **** char ssd1306_WriteChar(char ch, FontDef Font, SSD1306_COLOR color) {
 223:lib/ssd1306/ssd1306.c ****     uint32_t i, b, j;
 224:lib/ssd1306/ssd1306.c ****     
 225:lib/ssd1306/ssd1306.c ****     // Check if character is valid
 226:lib/ssd1306/ssd1306.c ****     if (ch < 32 || ch > 126)
 227:lib/ssd1306/ssd1306.c ****         return 0;
 228:lib/ssd1306/ssd1306.c ****     
 229:lib/ssd1306/ssd1306.c ****     // Check remaining space on current line
 230:lib/ssd1306/ssd1306.c ****     if (SSD1306_WIDTH < (SSD1306.CurrentX + Font.FontWidth) ||
 231:lib/ssd1306/ssd1306.c ****         SSD1306_HEIGHT < (SSD1306.CurrentY + Font.FontHeight))
 232:lib/ssd1306/ssd1306.c ****     {
 233:lib/ssd1306/ssd1306.c ****         // Not enough space on current line
 234:lib/ssd1306/ssd1306.c ****         return 0;
 235:lib/ssd1306/ssd1306.c ****     }
 236:lib/ssd1306/ssd1306.c ****     
 237:lib/ssd1306/ssd1306.c ****     // Use the font to write
 238:lib/ssd1306/ssd1306.c ****     for(i = 0; i < Font.FontHeight; i++) {
 239:lib/ssd1306/ssd1306.c ****         b = Font.data[(ch - 32) * Font.FontHeight + i];
 240:lib/ssd1306/ssd1306.c ****         for(j = 0; j < Font.FontWidth; j++) {
 241:lib/ssd1306/ssd1306.c ****             if((b << j) & 0x8000)  {

 243:lib/ssd1306/ssd1306.c ****             } else {
 244:lib/ssd1306/ssd1306.c ****                 ssd1306_DrawPixel(SSD1306.CurrentX + j, (SSD1306.CurrentY + i), (SSD1306_COLOR)!col
 245:lib/ssd1306/ssd1306.c ****             }
 246:lib/ssd1306/ssd1306.c ****         }
 247:lib/ssd1306/ssd1306.c ****     }
 248:lib/ssd1306/ssd1306.c ****     
 249:lib/ssd1306/ssd1306.c ****     // The current space is now taken
 250:lib/ssd1306/ssd1306.c ****     SSD1306.CurrentX += Font.FontWidth;
 251:lib/ssd1306/ssd1306.c ****     
 252:lib/ssd1306/ssd1306.c ****     // Return written char for validation
 253:lib/ssd1306/ssd1306.c ****     return ch;
 254:lib/ssd1306/ssd1306.c **** }
